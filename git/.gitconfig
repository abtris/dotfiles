[alias]
    st = status
    ci = commit
    br = branch
    co = checkout
    df = diff
    lg = log -p
    changes=diff --name-status -r
    diffstat=diff --stat -r
    upstream = log --oneline master..origin/master
	pending = log --oneline origin/master..master	
    myworklog = log --author=Ladislav --since=1.month --pretty='%h %Cgreen%ad %Cred%an %Cblue%s%Creset'
	worklog = log --pretty='%h %ad %an %s'
	worklogexport = log --pretty='%hÍ„|%at|%an|%s'
[color]
    ui = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = cyan        
[diff "doc"]
	textconv = antiword
[giggle]
	compact-mode = true
	main-window-maximized = true
	main-window-geometry = 1100x892+392+119
